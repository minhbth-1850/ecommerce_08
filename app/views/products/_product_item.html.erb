<tr>
  <td><%= image_tag product_item.image.url, size: "50x50" %></td>
  <td><%= product_item.id %></td>
  <td><%= product_item.name %></td>
  <td class="text-left"><%= product_item.info %></td>
  <td><%= product_item.category.name %></td>
  <td><%= product_item.quantity %></td>
  <td class="text-left"><%= product_item.get_price %></td>
  <td><%= product_item.rank %></td>
  <td class="text-right">
    <%= link_to "#", class: "btn btn-sm btn-warning" do %>
      <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
    <% end %>
    <%= link_to "#", class: "btn btn-sm btn-danger" do %>
      <span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
    <% end %>
  </td>
</tr>
